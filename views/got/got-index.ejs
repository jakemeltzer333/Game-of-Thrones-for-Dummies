<% include ../partials/boilerplate %>
<div class = 'got-container'>
  <!--Render's user's username to add personal touch to their lists-->
  <h1 class = 'header'><%= user.username %>'s Character List</h1>
  <div class = 'got'>

    <% for (let got of data) { %>
        <h2 class = 'name' id = '<%= got.id %>'><%= got.name %></h2>
        <a class = 'linkto' href='#'>See More</a>
        <form class = 'delete' method = 'POST' action = '/got/<%= got.id %>?_method=DELETE'>
          <input class = 'remove' type='submit' value='Delete (Valar Morghulis)'>
        </form>
   <% } %>
  </div>
    <a class = 'add' href='/got/add'>Add New Character</a>
    <a class = 'logout' href='auth/logout'>Until Next Time...The Night
    is Dark and Full of Terrors</a>
</div>

<% include ../partials/end %>
